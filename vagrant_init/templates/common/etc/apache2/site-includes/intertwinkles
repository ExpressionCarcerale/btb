Alias robots.txt /var/intertwinkles/static/robots.txt
Alias favicon.ico /var/intertwinkles/static/favicon.ico
Alias /static /var/intertwinkles/static/
<Directory /var/intertwinkles/static/>
    Order deny,allow
    Allow from all
</Directory>

Alias /media/ /var/intertwinkles/media/
<Directory /var/intertwinkles/media/>
    Order deny,allow
    Allow from all
</Directory>

WSGIDaemonProcess $INTERTWINKLES_DOMAIN_NAME display-name=wsgi-$INTERTWINKLES_DOMAIN_NAME
WSGIProcessGroup $INTERTWINKLES_DOMAIN_NAME
WSGIScriptAlias / /var/intertwinkles/intertwinkles/wsgi.py
<Directory /var/intertwinkles/intertwinkles/>
    <Files wsgi.py>
        Order allow,deny
        Allow from all
    </Files>
</Directory>
